<div class="jumbotron text-center">
  <h1>Photo's Ready!</h1>
</div>

<div class="items-list">
  <div class="row">
    <div class="col-md-12 text-center">
      <div class="well" id = "side-nav" >
        <h2><%= @store.name %></h2>
      </div>
      <div class="row item-info item-list">
        <%= render partial: "layouts/photo", collection: @photos %>
      </div>
      <div class="row text-center">
        <%= will_paginate @photos %>
      </div>
    </div>
  </div>
</div>
