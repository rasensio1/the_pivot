<%= render partial: "layouts/page_title", locals: { page_title: "Add a phtot" } %>
<%= render partial: "layouts/errors" %>
<div class="row">

  <div class="col-lg-12">

    <div class="admin-item-form">

      <%= form_for [:admin, @photo] do |f| %>
        <div class="form-group">
          <%= f.label :title %>
          <%= f.text_field :title, class: "form-control" %>
        </div>

        <%= f.submit "Create Photo", class: "btn btn-primary" %>
      <% end %>
    </div>
  </div>
</div>
